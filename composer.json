{
    "name": "kunicmarko/static-translations",
    "description": "This is a Symfony2 Bundle that helps you to generate translations for static strings from Excel files, thanks to the PHPExcel library",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Marko Kunic",
            "email": "kunicmarko20@gmail.com",
            "homepage": "http://www.kunicmarko.ml"
        }
    ],

    "keywords": ["static","translations", "symfony2", "bundle","symfony translation"],
    "homepage": "https://github.com/kunicmarko20/static-translation",
    "license": "MIT",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.8|~3.0",
        "phpoffice/phpexcel": "~1.8.1"
    },
    "autoload": {
        "exclude-from-classmap": ["/Tests/"],
        "psr-4": {
            "KunicMarko\\StaticTranslationBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KunicMarko\\StaticTranslationBundle\\": ""
        }
    }
}
